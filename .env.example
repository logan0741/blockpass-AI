# BlockPass AI OCR 환경 설정

# API 서버 설정
API_HOST=0.0.0.0
API_PORT=8000
DEBUG=false

# 모델 설정
DEVICE=cuda
TORCH_DTYPE=bfloat16
MAX_NEW_TOKENS=2048

# ngrok 설정 (이 서버를 외부에 노출할 경우)
NGROK_ENABLED=false
NGROK_AUTH_TOKEN=your_ngrok_auth_token_here

# DB 연결 설정 (백엔드 서버의 ngrok URL)
DB_NGROK_URL=https://your-backend-ngrok-url.ngrok.io
DB_API_KEY=your_api_key_here

# 업로드 설정
MAX_IMAGE_SIZE_MB=10
